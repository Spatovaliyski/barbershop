{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "title": "Barbershop Block",
  "description": "A block that lets users book available time",
  "textdomain": "tenup-theme",
  "name": "tenup/barbershop",
  "icon": "feedback",
  "category": "widgets", 
  "attributes":{
    "title": {
      "type": "string",
      "default": "Barbershop Block"
    },
    "services": {
      "type": "array",
      "default": [
        {
          "id": 1,
          "name": "Haircut",
          "price": 20
        },
        {
          "id": 2,
          "name": "Beard Trim",
          "price": 10
        },
        {
          "id": 3,
          "name": "Shave",
          "price": 15
        }
      ]
    }
  },
  "example": {
    "attributes":{
      "title": "Barbershop Block"
    }
  },
  "supports": {
    "html": false
  },
  "editorScript": "file:./index.js",
  "viewScript": "file:./view.js"
}
